   <children>
      <ScatterChart fx:id="scatterChart" layoutX="20.0" layoutY="23.0" prefHeight="557.0" prefWidth="862.0">
        <xAxis>
          <NumberAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </ScatterChart>
      <Button fx:id="saveChartButton" layoutX="398.0" layoutY="603.0" mnemonicParsing="false" onAction="#saveChart" text="Save Chart As Image" />
